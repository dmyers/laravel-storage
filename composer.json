{
    "name": "dmyers/laravel-storage",
    "description": "A simple storage filesystem package for Laravel 4.",
    "keywords": ["laravel", "storage", "filesystem"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Myers",
            "email": "arcticpro@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x"
    },
    "require-dev": {
        "aws/aws-sdk-php": "2.*"
    },
    "suggest": {
        "aws/aws-sdk-php": "Required for AmazonS3 driver.",
        "guzzlehttp/guzzle": "Required for remote uploads/downloads."
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Dmyers\\Storage": "src/"
        }
    },
    "minimum-stability": "dev"
}
